" plugin configs
execute pathogen#infect()
syntax on
filetype indent plugin on

" vim airline configs
let g:airline_powerline_fonts = 1
let g:airline_solarized_bg='dark'
set t_Co=256

" regular vim configs
set tabstop=4
set softtabstop=4
set shiftwidth=4
set smarttab
set autoindent
set expandtab
set number

colorscheme monokai




